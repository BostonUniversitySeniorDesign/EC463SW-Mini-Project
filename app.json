{
  "expo": {
    "name": "Food Tracker",
    "slug": "food-tracker",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/food.png",
    "splash": {
      "image": "./assets/food.png",
      "resizeMode": "cover",
      "backgroundColor": "#F57C00"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.Sands.CodeApp",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
